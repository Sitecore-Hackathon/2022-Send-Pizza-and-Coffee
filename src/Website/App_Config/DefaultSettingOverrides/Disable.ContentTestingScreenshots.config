<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
	<sitecore>
		<!-- 
			PERFORMANCE ENHANCER
			
			Screenshot Generation can happen on every Item save and can be very computationally expensive. Only
			turn it on if you're using it.
		-->
		<settings>
			<setting name="ContentTesting.GenerateScreenshots" value="none" />
		</settings>
	</sitecore>
</configuration>
