<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"  xmlns:env="http://www.sitecore.net/xmlconfig/env/">
	<sitecore>
		<settings>
			<!--
			ENABLE/DISABLE Tracking
			
			Sitecore defaults to having Tracking turned on. Early in a project this can cause
			quite a few headaches and impact Sitecore startup time. Use this file to disable Tracking
			until you're ready.
			
			-->
			<setting name="Xdb.Tracking.Enabled" value="false" env:require="Local" />
		</settings>
	</sitecore>
</configuration>
